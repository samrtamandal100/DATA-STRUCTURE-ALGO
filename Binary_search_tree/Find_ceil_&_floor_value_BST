Ceil//
public int ceil(node root ,int target)
   {
    		int ans =Integer.MIN_VALUE;
    		while(root!=null)
    		{
    			if(root.data==target)
    			{
    				return target;
    			}
    			if(root.data>target)
    			{   ans =root.data;
    				root =root.left;
    			}else
    			{
    				root=root.right;
    			}
    		}
    		return ans;
    }
    
    
   Floor// 
   public int ceil(node root ,int target)
    	{
    		int ans =Integer.MIN_VALUE;
    		while(root!=null)
    		{
    			if(root.data==target)
    			{
    				return target;
    			}
    			if(root.data>target)
    			{  
    				root =root.left;
    			}else
    			{
             ans =root.data;
    				root=root.right;
    			}
    		}
    		return ans;
    	}

